            <h3 style="color:{{ block.settings.color_1 }};"> {{ block.settings.tittle }} </h3> 
            {% endif %}
            {% if block.settings.text != blank %}
            <p style="color:{{ block.settings.color_1 }};">  {{ block.settings.text }} </p>
            {% endif %}

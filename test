              {{ block.settings.button-text }}
            </a>
            {% endif %}							

            {% if block.settings.featured_product-1 != blank %}
            {%- assign product = all_products[block.settings.featured_product-1] -%}        


            {%- assign collection_handles = product.collections | map: 'handle' -%}
            {% if product.empty? %}
            {%- assign section_onboarding = true -%}
            {% else %}
            {%- assign section_onboarding = false -%}
            {% endif %}
            <div itemprop="itemListElement" itemscope itemtype="http://schema.org/Product"
                 class="product-{{ product.id }} "
                 data-slideshow-animation="{{section.settings.slideshow_animation}}"
                 data-block-id="{{block.id}}"
                 {{ block.shopify_attributes }}>



              <div class="product-details half">

                <form class="product__form" id="product{{ product.id }}__form" action="/cart/add" method="POST">
                  <div class="product__variants">
                    {% if product.variants.size > 1 %}
                    <div class="form__control">
                      <div class="styled-select">
                        <select id="product-select-{{ product.id }}" name="id">
                          {% for variant in product.variants %}
                          <option {% if variant == product.selected_or_first_available_variant %} selected="selected" {% endif %} value="{{ variant.id }}" data-sku="{{ variant.sku }}">{{ variant.title }} - {{ variant.price | money }}</option>
                          {% endfor %}
                        </select>
                      </div>
                    </div>
                    {% else %}
                    <input type="hidden" name="id" data-sku="{{ product.selected_or_first_available_variant.sku }}" value="{{ product.selected_or_first_available_variant.id }}">
                    {% endif %}
                  </div>

                  <div class="product__buy">


                    {% if product.selected_or_first_available_variant.available %}
                    <button type="submit" style="color:{{ block.settings.color_1 }}; border-color:{{ block.settings.color_1 }};" class="button btn btn--regular pop-1 btn--outline btn--light button--primary button--no-disabled-opacity" {% if settings.cart_page != 'page' %}data-action="add-to-cart"{% endif %}>{{ 'product.general.add_to_cart' | t }}</button>
                    {% else %}
                    <button type="submit" class="button button--secondary button--no-disabled-opacity" disabled="disabled">{{ 'product.labels.sold_out' | t }}</button>
                    {% endif %}
                  </div>
                </form>
              </div>

            </div>

            {% endif %}	                  

          </div>
        </div>
      </div>
    </div>
  </div>
